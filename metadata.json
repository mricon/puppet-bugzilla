{
  "name": "mricon-bugzilla",
  "version": "0.1.0",
  "author": "Konstantin Ryabitsev",
  "summary": "Puppet module to manage bugzilla installation and configuration",
  "license": "Apache-2.0",
  "source": "https://github.com/lfit/puppet-bugzilla",
  "project_page": "https://github.com/lfit/puppet-bugzilla",
  "issues_url": "https://github.com/lfit/puppet-bugzilla/issues",
  "tags": [
    "bugzilla"
  ],
  "dependencies": [
    {"name":"puppetlabs-apache","version_requirement":">=1.10.0 <2.0.0"},
    {"name":"puppetlabs-git","version_requirement":">=0.1.0 <1.0.0"},
    {"name":"puppetlabs-mysql","version_requirement":">=3.0.0 <4.0.0"},
    {"name":"puppetlabs-stdlib","version_requirement":">= 4.11.0 <5.0.0"},
    {"name":"puppetlabs-vcsrepo","version_requirement":">=1.0.0 <2.0.0"},
    {"name":"puppetlabs-concat","version_requirement":">=1.0.0 <2.0.0"}
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">=4.0.0 <5.0.0"
    }
  ]
}
